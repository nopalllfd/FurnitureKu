    "components": {
        "schemas": {
            "Product": {
                "title": "Product",
                "required": [
                    "id",
                    "name",
                    "price",
                    "stock"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "Meja Belajar"
                    },
                    "slug": {
                        "type": "string",
                        "example": "meja-belajar"
                    },
                    "category_id": {
                        "type": "integer",
                        "example": 2
                    },
                    "description": {
                        "type": "string",
                        "example": "Meja kayu jati"
                    },
                    "price": {
                        "type": "number",
                        "format": "float",
                        "example": 120000
                    },
                    "stock": {
                        "type": "integer",
                        "example": 10
                    },
                    "image": {
                        "type": "string",
                        "example": "uploads/products/image.jpg"
                    },
                    "is_active": {
                        "type": "boolean",
                        "example": true
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer"
            }
        }
