        "/api/orders": {
            "get": {
                "tags": [
                    "Transactions"
                ],
                "summary": "Get the authenticated user's order history",
                "description": "Returns a list of orders for the logged in user.",
                "operationId": "955bb77a45ae173a485fadb8befed93f",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
